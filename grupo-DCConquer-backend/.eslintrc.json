{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["**/seeders", "**/migrations"],
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "no-await-in-loop": "off",
        "no-promise-executor-return": "off"
        
    }
  }
  